@model List<Car>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<div class="list-group">
    <ul class="list-group">
        @foreach (Car car in Model)
        {
            <li class="list-group-item">
                <a asp-controller="Car" asp-action="Index" asp-route-id="@car.id">
                    @car.Oil
                    @car.Year
                </a>
            </li>

        }
    </ul>
</div>

